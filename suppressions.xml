<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="JavadocPackage" files="java$"/>
    <suppress checks="JavadocType" files="java$"/>
    <suppress checks="JavadocVariable" files="java$"/>
    <suppress checks="JavadocMethod" files="java$"/>
    <suppress checks="DesignForExtension" files="java$"/>
    <suppress checks="MagicNumber" files="java$"/>
    <suppress checks="VisibilityModifier" files="java$"/>
    <suppress checks="MemberName" files="java$"/>
    <suppress checks="HideUtilityClassConstructor" files="java$"/>
    <suppress checks="ParameterNumber" files="java$"/>
    <suppress checks="EmptyForIteratorPad" files="java$"/>
    <suppress checks="EmptyStatement" files="java$"/>
    <suppress checks="EmptyBlock" files="java$"/>
    <suppress checks="FinalParameters" files="java$"/>
    <suppress checks="RegexpSingleline" files="java$"/>
    <suppress checks="NewlineAtEndOfFile" files="properties$"/>
    <suppress checks="RegexpMultiline" files="java$"/>
</suppressions>